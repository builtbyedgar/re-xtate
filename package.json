{
  "name": "re-xtate",
  "version": "0.1.3-alpha.0",
  "description": "A React state management made easy",
  "author": "Edgar Bermejo",
  "license": "MIT",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "source": "dist/index.ts",
  "types": "./types/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist/",
    "types/"
  ],
  "keywords": [
    "react",
    "state",
    "management"
  ],
  "homepage": "https://github.com/builtbyedgar/re-xtate",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/builtbyedgar/re-xtate.git"
  },
  "bugs": {
    "url": "https://github.com/builtbyedgar/re-xtate/issues"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run compile && npm run clean",
    "compile": "rollup -c",
    "clean": "rm -rf dist/core dist/hook dist/middleware dist/plugin dist/utils",
    "types": "tsc"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "peerDependencies": {
    "react": "^18.0"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/node": "^20.1.0",
    "@types/use-sync-external-store": "^0.0.3",
    "rollup": "^3.21.5",
    "rollup-plugin-dts": "^5.3.0",
    "tslib": "^2.5.0"
  },
  "dependencies": {
    "@redux-devtools/extension": "^3.2.5",
    "use-sync-external-store": "^1.2.0"
  }
}
